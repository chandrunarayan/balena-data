   "source": [
    "# first led circuit\n",
    "from gpiozero import LED\n",
    "import time\n",
    "\n",
    "# LED setup\n",
    "gpio_ledpin = 4\n",
    "led = LED(gpio_ledpin)\n",
    "\n",
    "while True:\n",
    "    led.on()\n",
    "    time.sleep(1)\n",
    "    led.off()\n",
    "    time.sleep(1)"
   ]
